void setup(){

size(600,600);
background(0);

}

void draw(){
  
 for(int i = 20; i > 10; i++){
   stroke(255,0,0);
   line(i,250,250,250);
   for(int j = 20; j > 10; j++){
     stroke(255,0,0);
     line(j,500,250,500);
     for(int k = 20; k > 10; k++){
       stroke(255,0,0);
       line(k,250,20,500);
       
 stroke(255,0,0);
 line(250,250,250,500);
     }
   }
 }
 
 
 
 
}
     
  
